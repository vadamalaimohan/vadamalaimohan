#include <stdio.h>

int main() {
    // While loop 
    int num=5;
    while (num>0)
    {
        printf("python\n");
        num--;
    }
    
}


//java in whil loop
#include <stdio.h>

int main() {
    // While loop 
    int num=0;
    while (num<5)
    {
        printf("java\n");
        num++;
    }
    return 0;
    
}
 
